<div class="card">
  <div class="card-body">
    <h5 class="card-title">{{ title|safe }}</h5>
    <h6 class="card-subtitle mb-2 text-muted">{{ subtitle|safe }}</h6>
    <p class="card-text">{{ text|safe }}</p>
  </div>
  <div class="card-footer">
  {% for key, link in links.items() %}
      <small class="text-muted">{{ link|safe }}</small>
  {% endfor %}
  </div>
</div>